<%= form_for @client do |f|%>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <br><br>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    <br><br>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <br><br>
    <%= f.label :phone %>
    <%= f.telephone_field :phone %>
    <br><br>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <br><br>
    <%= f.submit%>
<%end%>